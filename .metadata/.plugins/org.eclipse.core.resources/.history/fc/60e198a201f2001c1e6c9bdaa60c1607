import java.util.Scanner;

public class Sample {

	public static void main(String[] args) {
		Scanner sc = new Scanner(System.in);
	    Scanner sc1 = new Scanner(System.in);
	    int size = sc.nextInt(); 
	    int same=0;
	    int unique=0;
	    int[] arr = new int[size];
	    for(int i=0;i<size;i++){
	        arr[i]=sc1.nextInt();
	    }
	    for(int i=0;i<size;i++){
	    	if(arr[i]==arr[4]) {
	    		break;
	    	}
	        if(arr[i]==arr[i+1]){
	        same++;
	       }
	       else{
	        unique++;
	       }
	    }
	    System.out.println(unique);
	sc.close(); sc1.close();
	}

}
