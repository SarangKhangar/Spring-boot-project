<?xml version="1.0" encoding="UTF-8"?>
 
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://www.jboss.org/dtd/hibernate/hibernate-mapping-3.0.dtd">

<hibernate-mapping>
	<class name="com.zensar.bean.Employee" table="Employees">
		<id name="EmpId" column="e_id">
			<generator class="native"></generator>
		</id>
		<property name="Name" column="e_name"></property>
		<property name="dateOfJoining" column="e_doj"></property>
		<property name="Basic" column="e_basic"></property>
		<property name="Grade" column="e_grade"></property>
	</class>
</hibernate-mapping>